# ============================================
# Azure GenAI Accelerator - Terraform Variables
# ============================================

# Project Configuration
project_name = "genai"
location     = "westeurope"
environment  = "dev"

# LLM Provider Configuration
llm_provider = "openai"
openai_model = "gpt-4o"

# RAG Configuration
rag_enabled     = true
embedding_model = "text-embedding-3-small"

# Note: Sensitive variables should be provided via environment variables:
# - TF_VAR_db_admin_password
# - TF_VAR_openai_api_key
